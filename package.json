{
	"name": "@epicdesignlabs/gatsby-source-optimizely",
	"version": "4.1.0",
	"description": "This unofficial source plugin makes Optimizely API data available in GatsbyJS sites. Currently in active development.",
	"scripts": {
		"build": "babel ./src/ --out-dir ./ --source-maps",
		"commit": "git-cz",
		"watch": "babel ./src/ --out-dir ./ --source-maps --watch",
		"lint:eslint": "eslint \"./src/**/*.js\" --quiet --fix --color --ignore-path .gitignore",
		"lint:prettier": "prettier \"./src/**/*.js\" --write --ignore-path .gitignore",
		"lint": "yarn lint:eslint && yarn lint:prettier",
		"test": "yarn lint",
		"prepare": "cross-env NODE_ENV=production yarn build",
		"validate": "yarn lint",
		"ci:validate": "rm -rf node_modules && yarn install --frozen-lockfile && yarn validate",
		"prepublishOnly": "yarn ci:validate && yarn build"
	},
	"homepage": "https://github.com/Epic-Design-Labs/gatsby-source-optimizely/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Epic-Design-Labs/gatsby-source-optimizely.git"
	},
	"keywords": [
		"@epicdesignlabs",
		"api",
		"content",
		"delivery",
		"optimizely",
		"episerver",
		"gatsby-plugin",
		"gatsby-source-optimizely",
		"gatsby",
		"plugin",
		"source"
	],
	"author": "Epic Design Labs <info@epicdesignlabs.com> (https://epicdesignlabs.com)",
	"contributors": [
		{
			"name": "Guy Romelle Magayano",
			"email": "aspiredtechie2010@gmail.com",
			"url": "https://guyromellemagayano.dev"
		},
		{
			"name": "Max Jerry",
			"email": "maxjerry0107@gmail.com",
			"url": "https://www.maxjerry.com/"
		}
	],
	"license": "MIT",
	"main": "index.js",
	"dependencies": {
		"axios": "^1.2.2",
		"gatsby-source-filesystem": "^5.3.1",
		"lodash": "^4.17.21",
		"qs": "^6.11.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.19.3",
		"@babel/core": "^7.19.6",
		"@babel/eslint-parser": "^7.19.1",
		"@babel/plugin-transform-runtime": "^7.19.6",
		"@babel/runtime": "^7.20.0",
		"@commitlint/cli": "^17.2.0",
		"@commitlint/config-conventional": "^17.2.0",
		"babel-preset-gatsby-package": "^3.0.0",
		"babel-preset-minify": "^0.5.2",
		"commitizen": "^4.2.5",
		"cross-env": "^7.0.3",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "^8.31.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-babel": "^5.3.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.31.10",
		"gatsby-core-utils": "^4.1.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"react": "^18.2.0"
	},
	"peerDependencies": {
		"gatsby": ">4.0.0"
	},
	"bugs": {
		"url": "https://github.com/Epic-Design-Labs/gatsby-source-optimizely/issues"
	},
	"directories": {
		"src": "src"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn lint-staged --allow-empty",
			"prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
			"commit": "cz",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"**/*.{js,jsx,md,mdx,json}": [
			"yarn lint",
			"git add"
		]
	},
	"engines": {
		"node": ">=16.15.1"
	}
}
